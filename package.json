{
  "name": "traceprompt",
  "private": true,
  "version": "0.0.1",
  "description": "Tamper-proof AI logs for audit and compliance.",
  "workspaces": [
    "*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "test": "yarn workspaces run test",
    "dev": "yarn workspaces run dev",
    "build:sdk": "yarn workspace @traceprompt/sdk build",
    "test:sdk": "yarn workspace @traceprompt/sdk test",
    "dev:sdk": "yarn workspace @traceprompt/sdk dev",
    "publish:sdk": "yarn workspace @traceprompt/sdk publish-sdk",
    "rebuild:sdk": "yarn build:sdk && cd ../test && yarn install --force"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "UNLICENSED"
}
