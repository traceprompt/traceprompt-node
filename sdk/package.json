{
  "name": "@traceprompt/sdk",
  "version": "0.0.4",
  "description": "Traceprompt SDK for tamper-proof AI logs for audit & compliance.",
  "keywords": [
    "ai",
    "logging",
    "compliance",
    "audit",
    "tamper-proof",
    "traceprompt",
    "ai-safety",
    "llm",
    "annex-iv"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup",
    "test": "vitest run",
    "test:jest": "jest",
    "test:coverage": "vitest run --coverage",
    "dev": "tsc --watch",
    "watch": "tsup --watch",
    "link": "yarn link",
    "dev:test": "yarn build && yarn link && yarn watch",
    "publish-sdk": "yarn build && yarn test && npm publish --access public"
  },
  "dependencies": {
    "@napi-rs/blake-hash": "^1.3.4",
    "@opentelemetry/api": "^1.9.0",
    "axios": "^1.6.0",
    "openai": "^4.100.0",
    "zod": "^3.25.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@vitest/coverage-v8": "3.1.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.3"
  },
  "author": "Traceprompt",
  "license": "UNLICENSED"
}
